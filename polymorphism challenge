class Vehicle:
    def move(self):
        raise NotImplementedError("Subclasses must implement this method")

class Car(Vehicle):
    def move(self):
        print("Car is Driving ğŸš—")

class Plane(Vehicle):
    def move(self):
        print("Plane is Flying âœˆï¸")

class Boat(Vehicle):
    def move(self):
        print("Boat is Sailing ğŸš¤")

# --- Test ---
vehicles = [Car(), Plane(), Boat()]

for v in vehicles:
    v.move()  # Same method, different behavior (Polymorphism)
