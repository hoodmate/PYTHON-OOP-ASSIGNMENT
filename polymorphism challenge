class Vehicle:
    def move(self):
        raise NotImplementedError("Subclasses must implement this method")

class Car(Vehicle):
    def move(self):
        print("Car is Driving 🚗")

class Plane(Vehicle):
    def move(self):
        print("Plane is Flying ✈️")

class Boat(Vehicle):
    def move(self):
        print("Boat is Sailing 🚤")

# --- Test ---
vehicles = [Car(), Plane(), Boat()]

for v in vehicles:
    v.move()  # Same method, different behavior (Polymorphism)
